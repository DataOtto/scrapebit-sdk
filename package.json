{
  "name": "@scrapebit/sdk",
  "version": "1.0.0",
  "description": "Official Node.js SDK for Scrapebit - AI-powered web scraping API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "scrapebit",
    "web-scraping",
    "web-scraper",
    "scraper",
    "scraping",
    "data-extraction",
    "ai-scraping",
    "llm-scraping",
    "html-parser",
    "crawler",
    "automation",
    "pdf-generator",
    "screenshot",
    "headless-browser",
    "puppeteer-alternative",
    "playwright-alternative",
    "no-code-scraping",
    "structured-data",
    "api-client"
  ],
  "author": "Scrapebit <support@scrapebit.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DataOtto/scrapebit-sdk.git"
  },
  "homepage": "https://docs.scrapebit.com",
  "bugs": {
    "url": "https://github.com/DataOtto/scrapebit-sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
